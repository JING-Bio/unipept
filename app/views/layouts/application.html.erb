<!DOCTYPE html>
<html>
<head>
  <title><%=title%></title>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id='container'>
	<%= render 'layouts/topbar' %>
	<%= render 'layouts/header' %>
	<div id='breadcrumb' class='swapUnderline'>
		<span id='breadcrumbs-you-are-here'>You are here:</span> <a class='br-act' href='/index.php'>Home</a>
	</div>
	<div class='visualClear' id='clear-space-before-wrapper-table'>
		<!-- -->
	</div>
	<div id='main'>
		<%= render 'layouts/navigation' %>
		<nav id='navigationhome'>
			<div id='mainnav'>
				<ul>
					<li class=''>
						<a href='#'>Home</a>
					</li>
				</ul>
			</div>
			<div class='visualClear'></div>
		</nav>
		<section id='content' class='normal'>
			<div class='documentActions'>
				<ul>
					<li>
						<a href='javascript:this.print();'><img src='/images/print_icon.gif' alt='Print' title='Print' id='icon-print' /></a>
					</li>
				</ul>
			</div>
			<%= yield %>
		</section>
	</div>
</div>
<div class='visualClear'></div>
<div id='footer-wrap'>
	<%= render 'layouts/footer' %>
</div>
</body>
</html>